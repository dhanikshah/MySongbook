# Backend Environment Variables
# Copy this file to .env and fill in your values

# Server Port (Railway sets this automatically, but you can override)
PORT=3001

# OpenAI API Key (optional - for enhanced OCR)
OPENAI_API_KEY=your_openai_api_key_here

# Frontend URL (for CORS - set this to your frontend domain in production)
# Examples:
# FRONTEND_URL=https://my-songbook.vercel.app
# FRONTEND_URL=https://my-songbook.netlify.app
FRONTEND_URL=

# Custom Domain (if you're using a custom domain for the backend)
# CUSTOM_DOMAIN=https://api.mysongbook.com

# Node Environment
NODE_ENV=production

